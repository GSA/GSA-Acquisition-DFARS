      <title class="- topic/title ">
         <ph props="autonumber" class="- topic/ph ">ASSIGNMENTS_PART3</ph> —AIR FORCE ASSIGNMENTS</title>
      <conbody class="- topic/body  concept/conbody ">
         <table frame="none" colsep="0" rowsep="0" class="- topic/table ">
            <tgroup cols="2"
                    colsep="0"
                    rowsep="0"
