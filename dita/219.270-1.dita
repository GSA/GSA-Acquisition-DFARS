   <concept id="DFARS_219.270-1"
            ditaarch:DITAArchVersion="1.2"
            class="- topic/topic concept/concept ">
      <title class="- topic/title ">
         <ph props="autonumber" class="- topic/ph ">219.270-1</ph>
          Definition.
      </title>
      <conbody class="- topic/body  concept/conbody ">
         <p class="- topic/p ">As used in this section—</p>
         <p class="- topic/p ">“Nonprofit organization” means any organization that is—</p>
         <p outputclass="List2" class="- topic/p ">(1) Described in section 501(c) of the
Internal Revenue Code of 1986; and</p>
         <p outputclass="List2" class="- topic/p ">(2) Exempt from tax under section 501(a)
of that Code.</p>
      </conbody>
   </concept>
</dita>
